<?xml version="1.0"?>

<job_conf>
    <plugins>
        <plugin id="local" type="runner" load="galaxy.jobs.runners.local:LocalJobRunner" workers="4"/>
    </plugins>
    <destinations default="docker_local">
        <destination id="local" runner="local"/>
        <destination id="docker_local" runner="local">
          <param id="docker_enabled">true</param>
         <param id="docker_volumes">$defaults,/Users/ben/Projects/NaturalHistoryMuseum/SpecimenDataRefinery/SDR/galaxy-workflow/samples/images:/images:ro</param>
         <param id="docker_sudo">false</param>
         <param id="docker_run_extra_arguments">--entrypoint="" --env TEKLIA_MODEL_DECRYPTION_KEY="${TEKLIA_MODEL_DECRYPTION_KEY}"</param>           
	      </destination>
    </destinations>
</job_conf>
